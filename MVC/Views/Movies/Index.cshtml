@model IEnumerable<ApplicationCore.Models.MovieCardModel>

@{
ViewData["Title"] = "Movies";
}
<!DOCTYPE html>

<html>
<head>
  <title>title</title>
</head>
<body>
  <h1>Movies</h1>
    <div class="row">
      @foreach (var movie in Model)
      {
      <div class="col-md-3">
        @await Html.PartialAsync("_MovieCard", movie)
      </div>
      }
    </div>
</body>
</html>